<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>mylutece-franceconnect</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>0.9.2-SNAPSHOT</version>
    <description>module.mylutece.franceconnect.module.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>module.mylutece.franceconnect.module.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/mylutece/modules/franceconnect/franceconnect.png</icon-url>
    <copyright>Copyright 2001-2015 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>4.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
    <db-pool-required>0</db-pool-required>

   <!-- Applications -->
    <applications>
        <application>
            <application-id>franceconnect</application-id>
            <application-class>fr.paris.lutece.plugins.mylutece.modules.franceconnect.web.FranceConnectApp</application-class>
        </application>
    </applications>

    <!-- servlets -->
    <servlets>
        <servlet>
            <servlet-name>franceconnectAuthLogin</servlet-name>
            <url-pattern>/servlet/plugins/mylutece/modules/franceconnect/login</url-pattern>
            <servlet-class>fr.paris.lutece.plugins.mylutece.modules.franceconnect.web.OAuthLoginServlet</servlet-class>
        </servlet>
        
        <servlet>
            <servlet-name>franceconnectAuthLogout</servlet-name>
            <url-pattern>/servlet/plugins/mylutece/modules/franceconnect/logout</url-pattern>
            <servlet-class>fr.paris.lutece.plugins.mylutece.modules.franceconnect.web.OAuthLogoutServlet</servlet-class>
        </servlet>

    </servlets>
</plug-in>